{
	"name": "tabtalk",
	"version": "0.1.0",
	"description": "Javascript browser tab/window local message communication library.",
	"scripts": {
		"build": "rollup -c && npm run uglify",
		"docs": "jsdoc -d docs source",
		"uglify": "uglifyjs --compress --mangle --output build/tabtalk.min.js -- build/tabtalk.js"
	},
	"keywords": [
		"tab",
		"window",
		"message"
	],
	"author": "Tentone",
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"jsdoc": "^3.5.0",
		"rollup": "^1.0.0",
		"rollup-plugin-strip": "^1.2.1",
		"uglify-js": "^3.4.9"
	}
}
