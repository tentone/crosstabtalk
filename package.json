		"window",
		"message"
	],
	"author": "Tentone",
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"jsdoc": "^3.5.0",
		"rollup": "^1.0.0",
		"rollup-plugin-strip": "^1.2.1",
		"uglify-js": "^3.4.9"
	}
}
